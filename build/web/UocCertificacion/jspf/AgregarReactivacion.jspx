
<jsp:root version="2.0"
    xmlns:jsp="http://java.sun.com/JSP/Page" 
     xmlns:c="http://java.sun.com/jsp/jstl/core">
    
                    <article>
                        <h4>Agregar reactivaciopn</h4>
                    </article>
    
    
                    <article>
                        
                        <a href="javascript:void(0);" 
                           id="vinculo_reactivacion"
                           style="padding-left: 1em;"
                           > buscar reactivaciones
                        </a>       
                        
                    </article>
    
        
                    <article>    
                        
                        
                        <form class="ancho9" 
                                action="../UocCertificacion/Controlador/Agregar" 
                                method="post" 
                                id="uoca_form" 
                                name='uoca_form'
                                >
                                                        
                            <input  TYPE="hidden"
                                        name="presupuesto_id"  
                                        id="uoca_presupuesto_id"
                                        value="0"
                                        />                              

                            
                            <div class = "SinEnfoque" 
                                 id="pdsa_i3">                                
                                <label for="cc">cc:</label> 
                                <div class ="textbox">
                                    <input TYPE="text" 
                                           name="cc" 
                                           id="uoca_cc"                                           
                                           size ="100"  
                                           />                                
                                </div>            
                            </div>                            
                            
                            <div class = "SinEnfoque" 
                                 id="pdsa_i4">                                
                                <label for="llamado">llamado:</label> 
                                <div class ="textbox">
                                    <input TYPE="text" 
                                           name="llamado" 
                                           id="uoca_llamado"
                                           size ="100"  
                                           />                                
                                </div>            
                            </div>                            

                            <div class = "SinEnfoque" 
                                 id="pdsa_i5">                                
                                <label for="proveedor">proveedor:</label> 
                                <div class ="textbox">
                                    <input TYPE="text" 
                                           name="proveedor" 
                                           id="uoca_proveedor"
                                           size ="100"  
                                           />                                
                                </div>            
                            </div>                            
                            
                            <div class = "SinEnfoque" 
                                 id="pdsa_i6">                                
                                <label for="pac">pac:</label> 
                                <div class ="textbox">
                                    <input TYPE="text" 
                                           name="pac" 
                                           id="uoca_pac"
                                           size ="100"  
                                           />                                
                                </div>            
                            </div>       

                            <div class = "SinEnfoque" 
                                 id="pdsa_i7">                                
                                <label for="monto">monto:</label> 
                                <div class ="textbox">
                                    <input TYPE="text" 
                                           name="monto" 
                                           id="uoca_monto"
                                           class="right-aling"
                                           value ="0"
                                           />                                
                                </div>            
                            </div>    

                            <div class = "SinEnfoque" 
                                 id="pdsa_i8">                                
                                <label for="certificado">certificado:</label> 
                                <div class ="textbox">
                                    <input TYPE="text" 
                                           name="certificado" 
                                           id="uoca_certificado"
                                           class="right-aling"                                            
                                           value ="0"
                                           />                                
                                </div>            
                            </div>    


                            
                            <input  TYPE="hidden"
                                        name="tipo"  
                                        id="uoca_tipo"
                                        value="RA"
                                        />                                   
                            
                            
                        </form>	                                    
                        
                    </article>                       
    
    
    
                    <article class = "botones-footer">
    
                        <div id="busqueda_footer">

                            <div class ="commandbutton">
                                <a href="javascript:void(0);" 
                                   id ="uoca_agregar"
                                   class="botonEnlace" >Agregar</a>
                            </div>                                
                            
                            <div class ="commandbutton">
                                <a href="javascript:void(0);" 
                                   id ="uoca_cerrar"
                                   class="botonEnlace" >Cerrar</a>
                            </div>                                
                        
                     
                        </div> 
                               
                    </article>
    
    
</jsp:root>
                


