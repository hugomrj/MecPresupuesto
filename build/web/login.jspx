                            name="form_login"
                            id="form_login"
                            method="post"
                            >

                            <div class = "SinEnfoque" id="i1">
                                <label for="cuenta">Usuario:</label>
                                <div class ="textbox">
                                    <input TYPE="TEXT"
                                           name="cuenta"
                                           id ="cuenta"
                                           />
                                </div>
                            </div>

                            
                            <div class= "SinEnfoque" id="i2">
                                <label for="clave">Contrase√±a:</label>
                                <div class ="textbox">
                                    <input TYPE="PASSWORD"
                                           name="clave"
                                           id ="clave"
                                           />
